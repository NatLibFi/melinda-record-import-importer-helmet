{
	"name": "@natlibfi/melinda-record-import-importer-helmet",
	"description": "Helmet record importer for the Melinda record batch import system ",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/melinda-record-import-importer-helmet",
	"bugs": {
		"url": "https://github.com/natlibfi/melinda-record-import-importer-helmet/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/melinda-record-import-importer-helmet.git"
	},
	"license": "AGPL-3.0+",
	"version": "1.0.0",
	"main": "./dist/index.js",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"start": "node dist/index",
		"cli": "node dist/cli",
		"check-deps": "npm-check;exit 0",
		"lint": "eslint --fix src",
		"test": "npm run lint",
		"build": "babel src --source-maps --delete-dir-on-start --out-dir=dist --minified",
		"build:dev": "npm run build:transpile && npm run check-deps",
		"watch": "cross-env DEBUG=1 NODE_ENV=debug nodemon --watch src --exec 'npm test'"
	},
	"dependencies": {
		"@natlibfi/melinda-commons": "git+https://github.com/NatLibFi/melinda-commons-js.git#melkehitys-1541",
		"@natlibfi/melinda-record-import-commons": "8.0.0",
		"http-status": "^1.4.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.6",
		"@babel/node": "^7.8.7",
		"@babel/preset-env": "^7.9.6",
		"@babel/register": "^7.9.0",
		"@natlibfi/eslint-config-melinda-backend": ">1.0.0-alpha.1",
		"babel-eslint": "^10.1.0",
		"cross-env": "^7.0.2",
		"eslint": "^7.0.0",
		"nodemon": "^2.0.3",
		"npm-check": "^5.9.2"
	},
	"eslintConfig": {
		"extends": "@natlibfi/melinda-backend"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "12"
					}
				}
			]
		]
	}
}
